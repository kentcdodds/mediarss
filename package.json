{
  "name": "mediarss",
  "version": "2.0.1",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "imports": {
    "#app/*": "./app/*",
    "#test/*": "./test/*"
  },
  "scripts": {
    "dev": "NODE_ENV=development bun run --watch index.ts",
    "dev:test": "DATABASE_PATH=./data/test.db CACHE_DATABASE_PATH=./data/test-cache.db MEDIA_PATHS=audio:./local-test/1/audio NODE_ENV=development bun run --watch index.ts",
    "dev:test-mounted": "DATABASE_PATH=./data/mounted.db CACHE_DATABASE_PATH=./data/mounted-cache.db MEDIA_PATHS=audio-series:/Volumes/media/audio/series,audiobooks:/Volumes/media/audio/audiobooks NODE_ENV=development bun run --watch index.ts",
    "test": "bun test",
    "start": "NODE_ENV=production bun run index.ts",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "check": "biome check --write .",
    "check:ci": "biome check .",
    "validate": "bun run check:ci && bun run typecheck && bun run test"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/bun": "latest",
    "@types/semver": "^7.7.1",
    "semver": "^7.7.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@epic-web/cachified": "^5.6.0",
    "@epic-web/invariant": "^1.0.0",
    "@mcp-ui/server": "^5.16.3",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@remix-run/component": "^0.3.0",
    "@remix-run/fetch-router": "^0.15.0",
    "@remix-run/fs": "^0.4.0",
    "@remix-run/headers": "^0.19.0",
    "@remix-run/html-template": "^0.3.0",
    "@remix-run/interaction": "^0.4.0",
    "@remix-run/lazy-file": "^5.0.0",
    "@remix-run/logger-middleware": "^0.1.0",
    "@remix-run/mime": "^0.3.0",
    "@remix-run/response": "^0.3.0",
    "@remix-run/route-pattern": "^0.17.0",
    "@remix-run/static-middleware": "^0.4.1",
    "close-with-grace": "^2.4.0",
    "fast-sort": "^3.4.1",
    "get-port": "^7.1.0",
    "jose": "^6.1.3",
    "match-sorter": "^8.2.0",
    "music-metadata": "^11.10.4",
    "p-limit": "^7.2.0",
    "remix": "3.0.0-alpha.0",
    "zod": "^4.3.5"
  }
}
